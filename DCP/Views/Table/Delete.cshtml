@model DCP.ViewModel.TableVMs.TableVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.Connection.Name" />
<wt:display field="Entity.TableName" />
<wt:display field="Entity.IsPartitionTable" />
<wt:display field="Entity.CreateTimeColumnName" />
<wt:display field="Entity.UpdateTimeColumnName" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>