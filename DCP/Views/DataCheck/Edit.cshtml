@model DCP.ViewModel.DataCheckVMs.DataCheckVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.Name" />
<wt:combobox field="Entity.LeftTableID" items="AllLeftTables"/>
<wt:combobox field="Entity.RightTableID" items="AllRightTables"/>
<wt:combobox field="Entity.CheckGroupType" />
<wt:combobox field="Entity.RowChange" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>