@model DCP.ViewModel.DataCheckRunVMs.DataCheckRunVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.RunName" />
<wt:display field="Entity.StartedAt" />
<wt:display field="Entity.EndedAt" />
<wt:display field="Entity.PassedCaseCount" />
<wt:display field="Entity.FailedCaseCount" />
<wt:display field="Entity.Status" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>