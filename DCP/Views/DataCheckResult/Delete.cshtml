@model DCP.ViewModel.DataCheckResultVMs.DataCheckResultVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.DataCheck.Name" />
<wt:display field="Entity.DataCheckRun.RunName" />
<wt:display field="Entity.CreatedDate" />
<wt:display field="Entity.LeftCount" />
<wt:display field="Entity.RightCount" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>